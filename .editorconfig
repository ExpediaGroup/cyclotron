# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
indent_size = 4
max_line_length = off
trim_trailing_whitespace = false

[*.json]
indent_size = 2
max_line_length = off
trim_trailing_whitespace = false
